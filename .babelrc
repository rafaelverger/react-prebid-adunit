{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers": ["last 2 versions", "ie >= 9"]
      }
    }],
    "@babel/preset-react",
    "minify"
  ],
  "plugins": [
    "@babel/plugin-proposal-function-bind"
  ],
  "env": {
    "demo": {
      "plugins": [
        "react-hot-loader/babel"
      ]
    }
  }
}
